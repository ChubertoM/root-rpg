{
  "name": "root-rpg-server",
  "description": "root rpg server",
  "version": "0.0.0",
  "private": true,
  "main": "src",
  "author": {
    "name": "Kyle Kemp",
    "email": "kyle@seiyria.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^14.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run build",
    "lint": "eslint src/. --config .eslintrc.json --ext .ts --fix",
